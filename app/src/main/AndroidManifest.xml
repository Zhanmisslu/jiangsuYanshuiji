<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhan.heathmanage">
    <!-- 用于访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/healthmanage"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.LoginActivity">

        </activity>
        <activity android:name=".Main.MainActivity" />
        <activity android:name=".Main.Menu.SettingActivity" />
        <activity android:name=".Register.RegisterActivity" />
        <activity android:name=".Weclome.WelcomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register.Register2Activity" />
        <activity android:name=".Register.Register3Activity"></activity>
    </application>

</manifest>